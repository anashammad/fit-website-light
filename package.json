{
  "name": "fit-website",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "turbo": "^2.0",
    "prettier": "^3.2"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  }
}
